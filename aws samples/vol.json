{
  "Resources":
  {
    "vol1":
	{
	 "Type":"AWS::EC2::Volume",
	 "Properties":
	 {
	   "Size":"10",
	   "AvailabilityZone": "ap-south-1a",
	   "VolumeType":"gp2"
	 }
	},
	"myec2":
	{
	   "Type": "AWS::EC2::Instance",
	   "Properties":
	   {
	      "ImageId":"ami-0d773a3b7bb2bb1c1",
		  "InstanceType": "t2.micro",
		  "KeyName":"jan28"
	   }
	},
	"myeip":
	{
	   "Type": "AWS::EC2::EIP"
	},
	"ec2eip":
	{
	   "Type": "AWS::EC2::EIPAssociation",
	   "Properties":
	   {
	     "EIP": {"Ref":"myeip"},
		 "InstanceId":{"Ref":"myec2"}
		 
	   }
	}
  },
  "Outputs":
  {
     "myvolid":
	 {
	    "Value": {"Ref":"vol1"}
	 },
	 "publicip":
	 {
	   "Value": {"Fn::GetAtt":["myec2","PublicIp"]}
	 }
  }
}